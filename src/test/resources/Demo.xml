<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Demo Test Suite" parallel="tests" thread-count="2" >
  <test name="Login_Search_NY_Logout_Test">
    <groups>
      <run>
        <include name="login" />
        <include name="search" />
      </run>
    </groups>
    <classes>
      <class name="com.AutomationDemo.en.tests.Demo_Tests" />
    </classes>
  </test>
  <test name="Login_Logout_Test">
    <groups>
      <run>
        <include name="login" />
      </run>
    </groups>
    <classes>
      <class name="com.AutomationDemo.en.tests.Demo_Tests" />
    </classes>
  </test>
</suite>